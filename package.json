        "coverageDirectory": "./test-coverage"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "eslint": "^7.0.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "jest": "^26.0.1",
        "watch": "^1.0.2"
    },
    "dependencies": {
        "axios": "^0.19.2",
        "cheerio": "^1.0.0-rc.3",
        "discord.js": "^12.2.0",
        "puppeteer": "^3.0.2",
        "rewire": "^5.0.0"
    }
